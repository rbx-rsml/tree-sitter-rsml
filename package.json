{
    "name": "tree-sitter-rsml",
    "version": "0.1.2",
    "description": "Rsml grammar for tree-sitter",
    "repository": "https://github.com/tree-sitter/tree-sitter-rsml",
    "license": "MIT",
    "author": {
        "name": "Cameron P Campbell"
    },
    "main": "bindings/node",
    "types": "bindings/node",
    "keywords": [
        "incremental",
        "parsing",
        "tree-sitter",
        "rsml"
    ],
    "files": [
        "grammar.ts",
        "parse.js",
        "tree-sitter.json",
        "binding.gyp",
        "prebuilds/**",
        "bindings/node/*",
        "queries/*",
        "src/**",
        "*.wasm"
    ],
    "dependencies": {
        "@types/node": "^24.0.10",
        "node-addon-api": "^8.2.1",
        "node-gyp-build": "^4.8.2",
        "tree-sitter": "^0.25.0",
        "typescript": "^5.8.3"
    },
    "devDependencies": {
        "prebuildify": "^6.0.1",
        "tree-sitter-cli": "^0.25.6"
    },
    "scripts": {
        "install": "node-gyp-build",
        "prestart": "tree-sitter build --wasm",
        "start": "tree-sitter playground",
        "test": "node --test bindings/node/*_test.js",
        "generate": "tsc grammar.ts --skipLibCheck && tree-sitter generate",
        "example": "tree-sitter parse example.rsml -c"
    }
}
